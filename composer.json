{
  "name": "dmp/rest-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "DMP Rest bundle",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "sensio/framework-extra-bundle": "^6.2",
    "symfony/serializer-pack": "*",
    "symfony/framework-bundle": "*"
  },
  "require-dev": {
    "dmp/cg-lib": "^0.1.1",
    "doctrine/annotations": "^1.13",
    "doctrine/collections": "^1.8",
    "doctrine/common": "^3.4",
    "doctrine/dbal": "^2.13",
    "doctrine/orm": "^2.13",
    "matthiasnoback/symfony-dependency-injection-test": "^4.3",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "*",
    "symfony/console": "*",
    "symfony/messenger": "*",
    "symfony/http-kernel": "*",
    "symfony/validator": "*",
    "symfony/routing": "*",
    "symfony/dependency-injection": "*",
    "symfony/config": "*",
    "symfony/options-resolver": "*",
    "symfony/web-link": "*"
  },
  "autoload": {
    "psr-4": {
      "DMP\\RestBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMP\\RestBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-cc": "phpunit coverage-html report/"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  }
}
